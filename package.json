{
  "name": "seleniumkit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@root": "file:./",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^94.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "selenium-webdriver": "^4.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "test": "mocha index.js --reporter nyan --recursive tests/",
    "test:hl": "set NODE_HEADLESS=true NODE_PATH=. && mocha --recursive tests/ --reporter nyan",
    "test:ci": "mocha --recursive tests/ --reporter json > test-results.json"
